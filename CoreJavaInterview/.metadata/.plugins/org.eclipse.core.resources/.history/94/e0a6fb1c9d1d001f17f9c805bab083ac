package com.rays.first;

public class abc {

	public static void main(String[] args) {
		String[] name = { "abc", "cde" };
		// int count = 0;

		for (String s : name) {

			for (char a = 'a'; a <= 'z'; a++) {
				for (int i = 0; i < s.length(); i++) {
					if (a == s.charAt(i)) {
						count++;

					}
				}

				if (count != 0) {
					System.out.print(a + " " + count + ",");
					count = 0;
				}
			}
		}
	}
}
