package com.rays.first;

public class abc {

	public static void main(String[] args) {
		String[] name = { "abc", "cde" };
		int count = 0;
		String str = null;
		for (String s : name) {
			str = s;
			for (char a = 'a'; a <= 'z'; a++) {
				for (int i = 0; i < s.length(); i++) {
					if (a == str.charAt(i)) {
						count++;

					}
				}

				if (count != 0) {
					System.out.println(a + " " + count);
					count = 0;
				}
			}
		}
	}
}
